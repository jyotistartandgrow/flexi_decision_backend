  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "pg": "^8.9.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "@types/cors": "^2.8.17",
    "knex-cleaner": "^1.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1"
  }
}
