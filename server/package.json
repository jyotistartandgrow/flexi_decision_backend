{
  "name": "flexidecision-server",
  "version": "0.1.0",
  "main": "dist/src/app.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc -p .",
    "start": "node dist/src/app.js",
    "migrate": "knex --knexfile ./knexfile.ts migrate:latest",
